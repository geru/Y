/**
 * Implements hook_theme().
 */
function ylayout_theme($existing, $type, $theme, $path) {
  $ret = y__protocol('layout', 'theme'); // returns hook_theme info for all active layout definitions
  return $ret;
}

/**
 * Implements hook_y()
 * @return array
 */
function ylayout_y() {
  return array( 'layout' );
}

